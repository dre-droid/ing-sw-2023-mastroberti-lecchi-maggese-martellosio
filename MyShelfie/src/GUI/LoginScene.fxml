<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="LoginAnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@Application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoginSceneController">
   <children>
      <Label id="TopLabel" layoutX="109.0" layoutY="14.0" styleClass="open-sans" text="Insert your username" textFill="#f2eded">
         <font>
            <Font size="45.0" />
         </font>
      </Label>
      <Button id="LoginButton" layoutX="264.0" layoutY="279.0" mnemonicParsing="false" onAction="#switchToNextScene" text="Login">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TextField fx:id="usernameText" layoutX="210.0" layoutY="185.0" promptText="username" styleClass="open-sans" />
      <Label fx:id="InfoLabel" layoutX="209.0" layoutY="208.0" styleClass="open-sans">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
